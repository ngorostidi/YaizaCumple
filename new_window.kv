<Portada>:
  name: 'portada'
  
  canvas.before:
    Color:
      rgba: (123/255, 8/255, 1/255, 1)
    Rectangle:
      pos: self.pos
      size: self.size
      source: 'fondo.jpg'

  BoxLayout:
    orientation: 'vertical'
    size: root.width, root.height

    spacing: 20
    padding: 50

    Image:
      source: 'logo-cazador'

    Label:
      id: passw
      text: 'Contraseña:' 

    TextInput:
      id: name_input
      multiline: False

    BoxLayout:
      orientation: 'horizontal'
      size: root.width, 0.05
      spacing: 20

      Button:
        text: 'Introducir'
        size_hint: 0.2, 0.4
        on_press: root.press()

      Button:
        id: nex
        text: 'Continuar'
        size_hint: 0.2, 0.4
        disabled: True
        on_release: 
          app.root.current = 'ronda1intro'
          root.manager.transition.direction = 'left'


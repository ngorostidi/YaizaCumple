<Button>
  font_name: 'NotoSans-Bold'
  font_size: 28
  size_hint: 0.6, 0.4
  pos_hint: {'center_x': 0.5}
  disabled_color: (0, 0, 0, 0.8)
  background_disabled_normal: ''
  background_normal: ''
  background_color: (123/255, 8/255, 1/255, 1)
  canvas.before:
    Color:
      rgba: 0, 0, 0, 1
    Line:
      width:2
      rectangle: self.x, self.y, self.width, self.height

<RoundedButton@Button>
  font_name: 'NotoSans-Regular'
  font_size: 28
  size_hint: 0.6, 0.4
  pos_hint: {'center_x': 0.5}
  disabled_color: (0, 0, 0, 0.8)
  background_disabled_normal: ''
  background_normal: ''
  background_color: (123/255, 8/255, 1/255, 1)
  canvas.before:
    Color:
      rgba: 0, 0, 0, 1
    Line:
      width:2
      rounded_rectangle: (self.x, self.y, self.width, self.height, 50, 50, 50, 50)
  canvas.before:
    Color:
      rgba: 0, 0, 0, 1
    RoundedRectangle:
      size: self.size
      pos: self.pos
      radius: [20,] * 4

<TextInput>
  halign: 'center'
  valign: 'center'
  font_name: 'NotoSans-Bold'
  size_hint: 0.8, 0.2
  font_size: 20
  padding: 6, self.height/2-self.font_size/1.5, 6, 6
